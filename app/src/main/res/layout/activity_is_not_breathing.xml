<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".IsNotBreathingActivity">

    <TextView
        android:id="@+id/accident_activity_unconscious_Is_Not_Breathing_title"
        android:layout_width="350dp"
        android:layout_height="150dp"
        android:layout_marginTop="38dp"
        android:layout_marginBottom="593dp"
        android:gravity="center"
        android:text="Dla nieprzytomnego i nieoddychającego pacjenta wezwij karetkę, a następnie wykonaj dalsze instrukcje"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/accident_activity_unconscious_Is_Not_Breathing_get_emergency"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginTop="636dp"
        android:layout_marginBottom="45dp"
        android:onClick="switch_to_Emergency_call_activity"
        android:text="Wezwij pomoc"
        android:textColor="#424242"
        app:backgroundTint="#FF9800"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />



</androidx.constraintlayout.widget.ConstraintLayout>