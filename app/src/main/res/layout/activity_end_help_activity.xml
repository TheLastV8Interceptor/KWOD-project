<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EndHelpActivity">

    <TextView
        android:id="@+id/end_help_activity"
        android:layout_width="400dp"
        android:layout_height="200dp"
        android:layout_marginTop="241dp"
        android:layout_marginBottom="290dp"
        android:gravity="center"
        android:rotationY="0"
        android:scrollbarAlwaysDrawVerticalTrack="false"
        android:text="Zakończono proces wspierania udzielania pierwszej pomocy"
        android:textAppearance="@style/TextAppearance.AppCompat.Display4"
        android:textSize="36sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/next_test_button"
        android:layout_width="200dp"
        android:layout_height="80dp"
        android:layout_marginTop="599dp"
        android:layout_marginBottom="52dp"
        android:onClick="switch_to_MainActivity"
        android:text="PONÓW"
        app:backgroundTint="#FF9800"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>